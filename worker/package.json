{
	"name": "@cube-codes/cube-codes-ide-worker",
	"version": "1.0.0",
	"type": "commonjs",
	"scripts": {
		"build": "tsc",
		"test": "jest",
		"build-browser": "rollup -c",
		"build-docs": "typedoc",
		"build-editor-docs": "typedoc --options editor-docs/typedoc.json",
		"ci-build": "npm ci && npm run build && npm run test && npm run build-browser && npm run build-docs && npm run build-editor-docs",
		"ci-release": "npm run ci-build"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^16.0.0",
		"@rollup/plugin-node-resolve": "^11.2.0",
		"@types/jest": "^26.0.21",
		"jest": "^26.6.3",
		"rollup": "^2.42.1",
		"ts-jest": "^26.5.4",
		"typedoc": "^0.20.32",
		"typescript": "^4.2.3"
	},
	"dependencies": {
		"@cube-codes/cube-codes-model": "0.0.38"
	}
}
